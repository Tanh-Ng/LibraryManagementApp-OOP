      xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/23.0.1"
      fx:controller="com.librarymanagement.UI.TabMenuController">

    <Button styleClass="menu-button" text="Home Page" onAction="#handleToHomePageUser"/>
    <Button styleClass="menu-button" text="Borrow Document"/>
    <Button styleClass="menu-button" text="Return Document"/>
    <Button styleClass="menu-button" text="Borrowed Documents" onAction="#hanldeToBorrowedDocuments"/>
    <Button styleClass="menu-button" text="Logout" onAction="#handleLogout"/>
</VBox>
