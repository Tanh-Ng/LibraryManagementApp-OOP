<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="406.0" prefWidth="524.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.librarymanagement.UI.AccountSettingController">

    <children>
        <Label layoutX="197.0" layoutY="61.0" prefHeight="49.0" prefWidth="144.0" text="Change Password" textAlignment="CENTER">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="92.0" layoutY="150.0" text="Old password:">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="90.0" layoutY="198.0" text="New password:">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="37.0" layoutY="251.0" text="Re-enter new password:">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <PasswordField fx:id="oldPasswordField" layoutX="220.0" layoutY="147.0" prefHeight="25.0" prefWidth="250.0" />
        <PasswordField fx:id="newPasswordField" layoutX="220.0" layoutY="196.0" prefHeight="25.0" prefWidth="250.0" />
        <PasswordField fx:id="reEnterNewPasswordField" layoutX="220.0" layoutY="246.0" prefHeight="25.0" prefWidth="250.0" />
        <Button layoutX="310.0" layoutY="299.0" mnemonicParsing="false" prefHeight="65.0" prefWidth="146.0" text="Confirm" onAction="#handleChangePassword"/>
      <Button layoutX="81.0" layoutY="299.0" mnemonicParsing="false" prefHeight="65.0" prefWidth="154.0" text="Cancel" onAction="#handleCancel"/>
    </children>
</AnchorPane>
